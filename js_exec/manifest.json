{
	"manifest_version": 2,

	"name": "JS Exec",
	"description": "Inject your own JavaScript in any webpage.",
	"version": "1.0",

	"options_page": "options.html",

	"permissions": [
		"storage",
		"unlimitedStorage",
		"tabs",
		"windows",
		"http://*/*",
		"https://*/*"
	],

	"browser_action":
	{
		// "default_icon": "icon.png",
		"default_popup": "popup.html"
	}
}